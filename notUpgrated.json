{
  "name": "dreamfactory/dreamfactory",
  "description": "The DreamFactory(tm) Platform.",
  "keywords": [
    "api",
    "dreamfactory",
    "laravel",
    "rest"
  ],
  "homepage": "https://www.dreamfactory.com/",
  "license": "Apache-2.0",
  "type": "project",
  "authors": [
    {
      "name": "DreamFactory Team",
      "email": "code@dreamfactory.com"
    }
  ],
  "support": {
    "email": "dspsupport@dreamfactory.com",
    "source": "https://github.com/dreamfactorysoftware/dreamfactory",
    "issues": "https://github.com/dreamfactorysoftware/dreamfactory/issues",
    "wiki": "https://wiki.dreamfactory.com",
    "guide": "https://guide.dreamfactory.com"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/alexanderko/df-core.git"
    },
    {
      "type": "path",
      "url": "../df-git",
      "canonical": false
    },
    {
      "type": "path",
      "url": "../df-oauth",
      "canonical": false
    },
    {
      "type": "path",
      "url": "../df-email",
      "canonical": false
    },
    {
      "type": "path",
      "url": "../df-azure",
      "canonical": false
    },
    {
      "type": "path",
      "url": "../df-graphql",
      "canonical": false
    },
    {
      "type": "path",
      "url": "../df-mongo-logs",
      "canonical": false
    },
    {
      "type": "path",
      "url": "../df-aws",
      "canonical": false
    },
    {
      "type": "path",
      "url": "../df-file",
      "canonical": false
    }
  ],
  "require": {
    "php": "^8.1",
    "dreamfactory/df-admin-app": "~4.12.0",
    "dreamfactory/df-apidoc": "~0.7.2",
    "dreamfactory/df-aws": "~0.16.3",
    "dreamfactory/df-azure": "dev-php81 as 0.16.2",
    "dreamfactory/df-cache": "~0.12.2",
    "dreamfactory/df-cassandra": "~0.13.1",
    "dreamfactory/df-couchbase": "~0.11.1",
    "dreamfactory/df-couchdb": "~0.16.1",
    "dreamfactory/df-email": "dev-php81 as 0.10.2",
    "dreamfactory/df-exporter-prometheus": "~1.0.5",
    "dreamfactory/df-file": "~0.7.4",
    "dreamfactory/df-filemanager-app": "~0.3.2",
    "dreamfactory/df-firebird": "~0.8.1",
    "dreamfactory/df-git": "dev-php81 as 0.6.3",
    "dreamfactory/df-graphql": "dev-php81 as 0.4.0",
    "dreamfactory/df-mongo-logs": "dev-php81 as 1.1.2",
    "dreamfactory/df-oauth": "dev-php81 as 0.16.1",
    "dreamfactory/df-rackspace": "~0.15.1",
    "dreamfactory/df-rws": "~0.15.3",
    "dreamfactory/df-sqldb": "~0.17.5",
  },
  "require-dev": {

  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "DreamFactory\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "3.0.x-dev"
    },
    "installer-paths": {
      "public/{$name}/": [
        "type:dreamfactory-app"
      ]
    },
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "dreamfactory/installer": true
    }
  }
}
